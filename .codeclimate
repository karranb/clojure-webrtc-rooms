version: '2'
checks:
  complex-logic:
    config:
      threshold: 7
plugins:
  eslint:
    enabled: true
    config:
      config: .eslintrc.js
      channel: 'eslint-6'
      ignore_warnings: true
  duplication:
    enabled: true
    config:
      languages:
        javascript:
          mass_threshold: 50
exclude_patterns:
  - '**/node_modules/'